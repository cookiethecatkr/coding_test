class Solution {
    public String solution(String rsp) {
        String answer = "";
          for(int i = 0; i < rsp.length(); i++){
            char tmp = rsp.charAt(i);
            if (tmp == '2'){
                answer = answer +"0";
            }
    
            if (tmp == '0'){
                answer = answer + "5";
            }
    
            if (tmp == '5'){
                answer = answer + "2";
            }

        }
        return answer;
    }
}
